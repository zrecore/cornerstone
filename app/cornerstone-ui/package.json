{
  "name": "cornerstone-ui",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href \"/\" --host 0.0.0.0 --disable-host-check --watch",
    "build": "NODE_ENV=production ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && ng run pekin-ui:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "compile:server": "tsc -p server.tsconfig.json",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && NODE_ENV=production http-server",
    "serve:ssr": "NODE_ENV=production node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.0",
    "@angular/common": "7.0.0",
    "@angular/compiler": "7.0.0",
    "@angular/core": "10.2.5",
    "@angular/forms": "7.0.0",
    "@angular/http": "7.0.0",
    "@angular/platform-browser": "7.0.0",
    "@angular/platform-browser-dynamic": "7.0.0",
    "@angular/platform-server": "^7.0.0",
    "@angular/router": "7.0.0",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-utils/cookies": "^3.0.1",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/multer": "^1.3.7",
    "@types/nodemailer": "^4.6.5",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "express-validator": "^5.3.0",
    "helmet": "^3.14.0",
    "jquery": "^3.2.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "multer": "^1.4.1",
    "ngx-cookie": "^4.0.2",
    "nodemailer": "^4.6.8",
    "octicons": "^8.1.0",
    "popper.js": "^1.14.3",
    "rxjs": "^6.3.3",
    "tether": "^1.4.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "7.0.1",
    "@angular/compiler-cli": "7.0.0",
    "@angular/language-service": "7.0.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/jquery": "^3.3.21",
    "@types/mixpanel": "^2.14.0",
    "@types/node": "^10.12.0",
    "clean-css": "^4.2.1",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
